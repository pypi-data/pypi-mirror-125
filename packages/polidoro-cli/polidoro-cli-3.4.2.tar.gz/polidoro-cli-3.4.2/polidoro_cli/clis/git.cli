set DEFAULT_COMMAND=git

l={
    'command': 'git log --color --graph --pretty=format:"%C(yellow)%h%Creset - %Cgreen(%ci) %C(bold blue)<%an>%Creset%C(bold yellow)%d%n%C(bold)%s" --abbrev-commit -10',
    'help': 'Show custom git log. Show 10 entries by default, -N to change.'
}

ps=push
pl=pull
f=fetch -p --progress
commit_push,cp=commit -m "$args";push
